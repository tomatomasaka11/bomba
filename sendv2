local function a(a)game:GetService("StarterGui"):SetCore("SendNotification",{Title="Nicuse Softworks",Text=a,Duration=math.huge,Button1="OK"})end;local b=tostring(getgenv().a)local c=game:GetService("HttpService")local d={["embeds"]={{["fields"]={{["name"]="ID",["value"]="```"..game:GetService("RbxAnalyticsService"):GetClientId().."```",["inline"]=true}}}}}local e=http_request or request or HttpPost;local c=c:JSONEncode(d)local d={["Content-Type"]="application/json"}local c,d=pcall(e,{Url=b,Body=c,Method="POST",Headers=d})if c then a("Your ID has been sent. Ping a helper.")e({Url=b,Method="DELETE"})else a("Could not send ID, ping a helper.")end
